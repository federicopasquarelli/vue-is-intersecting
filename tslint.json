{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "curly": false,
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "interface-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false,
        "no-shadowed-variable": false,
        "max-line-length": false,
        "semicolon": [
            true,
            "never"
        ],
        "no-console": false
    }
}