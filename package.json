{
  "name": "vue-is-intersecting",
  "version": "0.1.3",
  "description": "a vuejs directive to execute a callback when the binding element is intersecting the viewport",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:min && npm run build:dts",
    "build:min": "rollup -c build/rollup.config.base.js",
    "build:dts": "tsc src/index.ts -d --emitDeclarationOnly --declarationDir dist/types",
    "lint": "tslint --project . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/federicopasquarelli/vue-is-intersecting.git"
  },
  "keywords": [
    "Vue",
    "Vuejs",
    "IntersectionObserver"
  ],
  "author": "deafhoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/federicopasquarelli/vue-is-intersecting/issues"
  },
  "homepage": "http://verylazyloading.herokuapp.com/",
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "rollup": "^2.53.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "rollup-plugin-uglify": "^6.0.4"
  }
}