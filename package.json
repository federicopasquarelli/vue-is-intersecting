{
  "name": "vue-is-intersecting",
  "version": "0.0.8",
  "description": "a vuejs directive for launching callbacks when an element is intersecting the viewport",
  "main": "dist/index.iife.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "rollup  -c build/rollup.config.iife.js -w",
    "build": "npm run build:iife && npm run build:esm && npm run build:umd && npm run build:dts",
    "build:iife": "rollup -c build/rollup.config.iife.js",
    "build:esm": "rollup -c build/rollup.config.esm.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "build:dts": "tsc src/index.ts -d --emitDeclarationOnly --declarationDir dist/types",
    "lint": "tslint --project . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/federicopasquarelli/vue-is-intersecting.git"
  },
  "keywords": [
    "Vue",
    "Vuejs",
    "IntersectionObserver"
  ],
  "author": "deafhoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/federicopasquarelli/vue-is-intersecting/issues"
  },
  "homepage": "http://verylazyloading.herokuapp.com/",
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "rollup": "^2.53.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {}
}